
<div id="back">
<div id="mainSection">
    <header>
        <h1>Hello {{dataValues.firstname}} {{dataValues.lastname}}!
            <br> Welcolme to Billify</h1>
    </header>
    <section id="lists">
        <div id="bills">
            <h3>Here is a list of your bills</h3>
            <div id="billList">
                <div id="dynamicBills">

                </div>
            </div>
        </div>
        <br>
    </section>
    <br>
    <section id="addSection">
        <div id="addBill">
            <span>
                <h3>Add a new bill</h3>
            </span>
            <form id="newBill" name="newBill" method="post" action="/newBill">
                <label for="payee">Who are you paying?</label>
                <br>
                <input class="text" name="payee" type="text" />
                <br>
                <label for="amountDue">How much are you paying?</label>
                <br>
                <input class="text" name="amountDue" type="text" />
                <br>
                <label for="category">What category is this bill?</label>
                <br>
                <select name="category">
                    <option value="Housing">Housing</option>
                    <option value="HousingIns">Housing Insurance</option>
                    <option value="Internet">Internet</option>
                    <option value="Power">Power</option>
                    <option value="Water">Water</option>
                    <option value="OtherUlt">Other Utilities</option>

                    <option value="Food">Monthly Food</option>

                    <option value="Car">Car</option>
                    <option value="CarIns">Car Insurance</option>
                    <option value="Gas">Gas</option>
                    <option value="HealthIns">Health Insurance</option>
                    <option value="Credit">Credit Cards</option>
                    <option value="Phone">Phone</option>



                    <option value="Entertainment">Entertainment</option>
                    <option value="Children">Costs associated with Children </option>

                    <option value="Other">Other</option>



                </select>

                <br> {{!--
                <input name="category" list="category" /> --}}
                <br>
                <label for="dateDue">What day of the month is this bill due?</label>
                {{!-- Will try to add calander --}}
                <br>
                <input class="text" name="dateDue" type="text" />
                <br>
                <label for="websiteAccess">Do you have a link to your bill? Paste it here for reference</label>
                <br>
                <input class="text" name="websiteAccess" type="text" />
                <br>
                <label for="notes">Do you have any notes for this bill?</label>
                <br>
                <input class="text" name="notes" type="text" />
                <br>
                <input type='hidden' value={{{dataValues.id}}} name="UserId" />
                <input class="btn" type="submit" value="Add a Bill" />
            </form>
        </div>
        <br>
        <div id="addExps">
            <span>Add a Purchase</span>
            <br>
            <form id="newExp" name="newExp" method="post" action="/newExp">
                <label for="payee">New Purchase</label>
                <br>
                <input class="text" name="payee" type="text" />
                <br>
                <label for="amount_paid">Amount Paid</label>
                <br>
                <input class="text" name="amount_paid" type="text" />
                <br>
                <label for="category">Category</label>
                <br>
                <input class="text" name="category" type="text" /> {{!-- will make drop down --}}
                <br>
                <label for="notes">Notes</label>
                <br>
                <input class="text" name="notes" type="text" />
                <br>
 <input type='hidden' value={{{dataValues.id}}} name="UserId" />
                <input class="btn" type="submit" value="585" />
            </form>
        </div>
    </section>
    <section id="graphs">
        <div id="billsGraph">
            <span>
                <h4>What your bills look like</h4>
            </span>
            {{!-- bill graph will go here --}}
        </div>
        <div id="expsGraph">
            <span>
                <h4>What your expenses look like</h4>
            </span>
            {{!-- exps graph will go here --}}
        </div>
        <div id="totalGraph">
            <span>
                <h4>What your total spendings look like</h4>
            </span>
            {{!-- totals will go here --}}
        </div>
    </section>
</div>
<div id="logout">
    <a href='/logout'>
        <button>Log Out</button>
    </a>
</div>
</div>
